{
  "name": "@vaxee/nuxt",
  "version": "0.8.2",
  "description": "Nuxt Module for Vaxee",
  "keywords": [
    "vaxee",
    "vue",
    "vuex",
    "store",
    "pinia",
    "state",
    "nuxt"
  ],
  "homepage": "https://vaxee.letstri.dev",
  "bugs": {
    "url": "https://github.com/letstri/vaxee/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/letstri/vaxee.git"
  },
  "license": "MIT",
  "author": {
    "name": "Valerii Strilets",
    "email": "valerii.strilets@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run dev:prepare && nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub . && nuxi prepare playground",
    "test:types": "pnpm dev:prepare && nuxi typecheck"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.0",
    "vaxee": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.8.3",
    "@nuxt/schema": "^3.13.0",
    "@nuxt/test-utils": "^3.14.1",
    "nuxt": "^3.13.0",
    "typescript": "^5.5.4",
    "vue-tsc": "^2.1.2"
  },
  "peerDependencies": {
    "vue": ">3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
